<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<GridPane xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="Mecanica.janelas.JanelaCadConserto"
          prefHeight="400.0" prefWidth="600.0" vgap = "5" hgap = "5">
    <padding>
        <Insets top="10" bottom="10" left="10" right="10"/>
    </padding>
    <Label text="Descrição:" GridPane.columnIndex="0" GridPane.rowIndex="0" labelFor="$tfDescricao"/>
    <TextField fx:id="tfDescricao" GridPane.columnIndex="1" GridPane.rowIndex="0"/>

</GridPane>
