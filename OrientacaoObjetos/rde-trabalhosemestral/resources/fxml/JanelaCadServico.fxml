<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<GridPane xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="Mecanica.janelas.JanelaCadServico"
          prefHeight="400.0" prefWidth="600.0" vgap = "50" hgap = "35">
    <padding>
        <Insets top="10" bottom="10" right="10" left="15" />
    </padding>
        <Button text="Cadastrar Pitura" GridPane.rowIndex="1" GridPane.columnIndex="5" onAction="#mostrarCadastroPintura"/>
        <Button text="Cadastrar Conserto" onAction="#mostrarCadastroConserto" GridPane.rowIndex="2" GridPane.columnIndex="5" />
        <Button text="Voltar" onAction="#mostrarMenuPrincipal" GridPane.rowIndex="3" GridPane.columnIndex="5" />



</GridPane>
