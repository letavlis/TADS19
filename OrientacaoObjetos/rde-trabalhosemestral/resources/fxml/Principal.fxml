<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="Mecanica.janelas.Principal"
            prefHeight="400.0" prefWidth="500.0">
    <padding>
        <Insets top="10" bottom="10" left="10" right="10"/>
    </padding>
    <top>
        <HBox spacing="10">
            <padding>
                <Insets bottom="10"/>
            </padding>
            <Button text="Cadastrar Veiculo" onAction="#mostrarCadastroVeiculo"/>
            <Button text="Cadastrar Serviço" onAction="#mostrarCadastroServico" />
            <Button text="Executar Serviço" />
            <Button text="Emitir Relatório" onAction="#mostrarEmitirRelatorio"/>
        </HBox>
    </top>
    <!--
    <left>

    </left>
    -->
    <center>
        <HBox spacing="10">
            <VBox spacing="1">
                <Label text="Veiculos"/>
                <ListView fx:id="lstwVeiculo"/>
            </VBox>
          <VBox spacing="1">
                <Label text="Serviço"/>
                <ListView fx:id="lstwServicos"/>
            </VBox>
        </HBox>
    </center>

</BorderPane>
